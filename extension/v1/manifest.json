{
    "manifest_version": 2,
    "name": "Website Sandbox Viewer",
    "version": "1.0",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }
  